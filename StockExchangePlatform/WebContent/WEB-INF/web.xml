    <servlet-class>at.jku.ce.brokerplatform.getStocks</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>getStocks</servlet-name>
    <url-pattern>/getStocks</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>BuyStock</display-name>
